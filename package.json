{
  "name": "xcode-12-theme",
  "displayName": "Xcode Theme",
  "description": "Xcode Theme, ported to VS Code",
  "icon": "images/icon.png",
  "version": "5.0.0",
  "scripts": {
    "build": "vsce package"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "publisher": "mateocerquetella",
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MateoCerquetella/xcode-theme"
  },
  "contributes": {
    "themes": [
      {
        "label": "Xcode Classic (Dark)",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Classic Dark.json"
      },
      {
        "label": "Xcode Classic (Light)",
        "uiTheme": "vs",
        "path": "./themes/Xcode Classic Light.json"
      },
      {
        "label": "Xcode Low Key (Light)",
        "uiTheme": "vs",
        "path": "./themes/Xcode Low Key.json"
      },
      {
        "label": "Xcode Spartan (Light)",
        "uiTheme": "vs",
        "path": "./themes/Xcode Spartan.json"
      },
      {
        "label": "Xcode Civic (Dark)",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Civic.json"
      },
      {
        "label": "Xcode Default (Dark)",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Default Dark.json"
      },
      {
        "label": "Xcode Default (Dark Customized Version)",
        "uiTheme": "vs-dark",
        "path": "./themes/Xcode Default Dark Customized Version.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.9.0"
  }
}
